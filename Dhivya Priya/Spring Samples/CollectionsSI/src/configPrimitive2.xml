<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<!-- -->
	<bean id="ob" class="com.aspire.collectionsPrimitive.Library">
		<property name="libraryName" value="Library"></property>
		<property name="bookTopics">
			<list>
				<value>Java</value>
				<value>Spring</value>
				<value>Servlet</value>
				<value>Javascript</value>
			</list>
		</property>
		<property name="authors">
			<set>
				<value>Ken Arnold</value>
				<value>James Gosling</value>
				<value>Kathy Sierra</value>
			</set>
		</property>
		<property name="books">
			<map>
				<entry key="12451" value="Ken Arnold"></entry>
				<entry key="21234" value="James Gosling"></entry>
				<entry key="85143" value="Kathy Sierra"></entry>
			</map>
		</property>
	</bean>
</beans>