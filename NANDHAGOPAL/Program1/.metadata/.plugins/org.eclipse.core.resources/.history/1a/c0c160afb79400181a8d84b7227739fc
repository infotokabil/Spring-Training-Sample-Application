package com.aspire.constructor;

public class Address {
	

	private int id;
	private int id1;

	private String city;
	
	private String country;

	public Address(int id, int id1, String city, String country) {
		System.out.println("Address construtor");
		this.id1=id1;
		this.id = id;
		this.city = city;
		this.country = country;
	}
	
	public int getId() {
		return id;
	}
	
	public void setId(int id) {
		this.id = id;
	}
	

	public String getCity() {
		return city;
	}

	public void setCity(String city) {
		this.city = city;
	}

	public String getCountry() {
		return country;
	}

	public void setCountry(String country) {
		this.country = country;
	}

	@Override
	public String toString() {
		return "Address [id=" + id + ", id1=" + id1 + ", city=" + city + ", country=" + country + "]";
	}

	
	
}
